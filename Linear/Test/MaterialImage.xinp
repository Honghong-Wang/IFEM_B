<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<simulation>

  <!-- General - geometry definitions !-->
  <geometry dim="2">
    <patchfile>quarter-disk-1patch.g2</patchfile>
    <refine u="7" v="7"/>
    <topologysets>
      <set name="bottom" type="edge">
        <item patch="1">1</item>
      </set>
      <set name="left" type="edge">
        <item patch="1">2</item>
      </set>
      <set name="model" type="face">
        <item patch="1"/>
      </set>
    </topologysets>

  </geometry>

  <!-- General - boundary conditions !-->
  <boundaryconditions>
    <dirichlet set="left" comp="12"/>
    <neumann set="bottom" direction="1">1</neumann>
  </boundaryconditions>

  <!-- Problem specific block !-->
  <elasticity>
    <texturematerial set="model" file="refdata/white_square.png">
      <range min="0.5" max="1.0"  E="1e3" nu="0.4" rho="1"/>
      <range min="0"   max="0.5"  E="1e0" nu="0.3" rho="1"/>
    </texturematerial>
  </elasticity>

</simulation>
